{
  "name": "bedrock-foundation-sass",
  "version": "0.1",
  "main": "",
  "ignore": [],
  "dependencies": {
    "foundation": "zurb/bower-foundation",
    "ImageService": "~1.0.1"
  },
  "copy": [
    {
      "expand": true,
      "cwd": "bower_components/foundation/scss/",
      "src": [
        "**"
      ],
      "dest": "_styles/foundation-scss/"
    },
    {
      "src": [
        "bower_components/modernizr/modernizr.js"
      ],
      "dest": "_scripts/vendor/modernizr.js"
    },
    {
      "src": [
        "bower_components/ImageService/images.php"
      ],
      "dest": "public/img/images.php"
    },
    {
      "src": [
        "bower_components/ImageService/.htaccess"
      ],
      "dest": "public/img/.htaccess"
    },
    {
      "src": [
        "base_files/_settings.scss"
      ],
      "dest": "_styles/_settings.scss"
    },
    {
      "src": [
        "base_files/style.scss"
      ],
      "dest": "_styles/style.scss"
    },
    {
      "src": [
        "base_files/script.js"
      ],
      "dest": "_scripts/script.js"
    },
    {
      "src": [
        "base_files/.htaccess"
      ],
      "dest": "public/.htaccess"
    }
  ],
  "rename": {
  }
}
