{
  "name": "bedrock",
  "version": "0.1",
  "main": "",
  "ignore": [],
  "dependencies": {
    "jquery": "1.11.0",
    "modernizr": "~2.7.1",
    "normalize-css": "~3.0.0",
    "ImageService": "~1.0.1"
  },
  "copy": [
    {
      "src": [
        "bower_components/modernizr/modernizr.js"
      ],
      "dest": "_scripts/vendor/modernizr.js"
    },
    {
      "src": [
        "bower_components/jquery/dist/jquery.min.js"
      ],
      "dest": "public/js/vendor/jquery-1.11.0.min.js"
    },
    {
      "src": [
        "bower_components/normalize-css/normalize.css"
      ],
      "dest": "public/css/vendor/normalize.css"
    },
    {
      "src": [
        "bower_components/ImageService/images.php"
      ],
      "dest": "public/img/images.php"
    },
    {
      "src": [
        "bower_components/ImageService/.htaccess"
      ],
      "dest": "public/img/.htaccess"
    }
  ],
  "rename": {
  }
}
