{
  "name": "bedrock-h5bp",
  "version": "0.1",
  "main": "",
  "ignore": [],
  "dependencies": {
    "html5-boilerplate": "",
    "ImageService": "~1.0.1"
  },
  "copy": [
    {
      "expand": true, 
      "cwd": "bower_components/html5-boilerplate/css/",
      "src": [
        "*"
      ],
      "dest": "public/css/"
    },
    {
      "expand": true,
      "cwd": "bower_components/html5-boilerplate/js/",
      "src": [
        "**"
      ],
      "dest": "public/js/"
    },
    {
      "src": [
        "bower_components/html5-boilerplate/.htaccess"
      ],
      "dest": "public/.htaccess"
    },
    {
      "src": [
        "bower_components/html5-boilerplate/crossdomain.xml"
      ],
      "dest": "public/crossdomain.xml"
    },
    {
      "src": [
        "bower_components/html5-boilerplate/humans.txt"
      ],
      "dest": "public/humans.txt"
    },
    {
      "src": [
        "bower_components/html5-boilerplate/robots.txt"
      ],
      "dest": "public/robots.txt"
    },
    {
      "src": [
        "bower_components/ImageService/images.php"
      ],
      "dest": "public/img/images.php"
    },
    {
      "src": [
        "bower_components/ImageService/.htaccess"
      ],
      "dest": "public/img/.htaccess"
    }
  ],
  "rename": {
  }
}
